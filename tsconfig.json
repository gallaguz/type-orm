{

   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "esnext.asynciterable"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "rootDir": "./src",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true
   },
   "include": ["src/**/*.ts"],
   "exclude": ["node_modules", "mongodb", "mysql", "postgres"]
}